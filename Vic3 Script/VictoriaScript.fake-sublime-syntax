  variable-definition-complex:
    - match: set_(local_|global_)?variable
      scope: keyword.effect
      push:
        - match: =
          scope: keyword.operator.comparison.vic
          push:
            - include: string-sub-start-def
            - match: ({{valid_variable}})
              scope: entity.name.function.var.declaration
              push:
                - include: comment-line-4
                - include: string-sub-end-def
                - match: \n
                  pop: 3
            - match: \{\n?+
              push:
                - match: \#.*$\n?
                  scope: comment.line
                - match: \}
                  pop: 4
                - match: name
                  scope: constant.numeric
                  push:
                    - match: =
                      scope: keyword.operator.comparison.vic
                    - match: \n
                      pop: 4
                    - include: comment-line-4
                    - include: string-sub-start-def
                    - match: ({{valid_variable}})
                      scope: entity.name.function.var.declaration
                      push:
                        - match: (#).*$\n?
                          scope: comment.line
                          pop: 5
                        - include: string-sub-end-def
                        - match: \n
                          pop: 5

  variable-usage-complex:
    - match: (change|round|clamp)_(local_|global_)?variable
      scope: keyword.effect
      push:
        - include: line_ending
        - match: =
          scope: keyword.operator.comparison.vic
        - match: \{\n?+
          push:
            - match: (#).*$\n?
              scope: comment.line
            - match: \}
              pop: 3

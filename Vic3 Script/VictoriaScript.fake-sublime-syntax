  comments:
    - match: \#.*$\n?
      scope: comment.line
      pop: true

  comment-line:
    - match: \#.*$\n?
      scope: comment.line
      pop: 2

  comment-line-4:
    - match: \#.*$\n?
      scope: comment.line
      pop: 4

  scripted-effect-arg:
    - match: \$
      scope: keyword.arg.start
      push:
        - match: ({{valid_variable}})
          scope: variable.language.scripted.arg
        - match: \$
          scope: keyword.arg.start
          pop: true
        - include: line_ending

  scripted-effect-zone:
    - match: \$
      scope: keyword.dolla
      push:
        - include: line_ending
        - match: ({{valid_variable}})
          scope: variable.language.scripted.arg
        - match: \$
          scope: keyword.dolla
          pop: 3

  string-sub-start:
    - match: \$
      scope: keyword.dolla
      push:
        - include: line_ending
        - match: ({{valid_variable}})
          scope: variable.language.scripted.arg
        - match: \$
          scope: keyword.dolla
          pop: 1

  string-sub-end:
    - match: \$
      scope: keyword.dolla
      push:
        - include: line_ending
        - match: ({{valid_variable}})
          scope: variable.language.scripted.arg
        - match: \$
          scope: keyword.dolla
          pop: 2

  string-sub-start-def:
    - match: \$
      scope: keyword.dolla
      push:
        - include: line_ending
        - match: ({{valid_variable}})
          scope: entity.name.scripted.arg
        - match: \$
          scope: keyword.dolla
          pop: 1

  string-sub-end-def:
    - match: \$
      scope: keyword.dolla
      push:
        - include: line_ending
        - match: ({{valid_variable}})
          scope: entity.name.scripted.arg
        - match: \$
          scope: keyword.dolla
          pop: 2

  scope-usage:
    - match: \bscope\b|\bvar\b|\blocal_var\b|\bglobal_var\b
      scope: storage.type.scope
      push:
        - include: line_ending_space
        - match: ':'
          scope: keyword.operator.comparison.vic
          push:
            - match: \n|\s
              pop: 2
            - include: comment-line
            - include: string-sub-start
            - match: ({{valid_variable}})
              scope: variable.parameter.scope.usage
              push:
                - include: comment-line-4
                - include: string-sub-end
                - match: \n|\s|\.
                  pop: 3

  remove-var:
    - match: remove_(local_|global_)?variable
      scope: keyword.effect
      push:
        - match: =
          scope: keyword.operator.comparison.vic
          push:
            - match: \n
              pop: 2
            - include: comment-line
            - include: string-sub-start
            - match: ({{valid_variable}})
              scope: variable.parameter.remove.var
              push:
                - include: comment-line-4
                - include: string-sub-end
                - match: \n
                  pop: 3

  save-scope:
    - match: save_scope_as
      scope: keyword.effect
      push:
        - match: =
          scope: keyword.operator.comparison.vic
          push:
            - match: \n
              pop: 2
            - include: comment-line
            - include: string-sub-start-def

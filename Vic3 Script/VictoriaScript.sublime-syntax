%YAML 1.2
---
name: Victoria Script
file_extensions: [txt]
scope: text.vic

variables:
  valid_variable: '[A-Za-z_][A-Za-z_0-9]*'

# IMPORTANT NOTE:
# Any changes made to this file should actually be made in VictoriaScript.fake-sublime-syntax
# This is so the plugin can generate the syntax file with dynamic content from mods/vanilla

contexts:
  main:
    - include: comments
    - include: at-symbol
    - include: at-symbol-expanded
    - include: scripted-effect-arg
    - include: variable-usage-complex
    - include: variable-definition-complex
    - include: save-scope
    - include: remove-var
    - include: one-line-var-trigger
    - include: scope-usage
    # Blocks
    - include: trigger-block-start
    - include: effect-block-start
    - include: ai-block-start
    - include: bg-block-start
    - include: char-trait-block-start

    - include: manually-entered-content
    - include: scripted-trigger-event-declaration
    - include: scripted-effect-event-declaration
    - include: auto-generated-content

  comments:
    - match: (#).*$\n?
      scope: comment.line

  scripted-effect-arg:
    - match: \$
      scope: keyword.arg.start
      push:
        - include: line_ending
        - match: ({{valid_variable}})
          scope: variable.language.arg
        - match: \$
          scope: keyword.arg.start
          pop: true

  scripted-effect-zone:
    - match: \$
      scope: keyword.dolla
      push:
        - include: line_ending
        - match: ({{valid_variable}})
          scope: variable.language.at.parameter
        - match: \$
          scope: keyword.dolla
          pop: 3


  scope-usage:
    - match: \bscope\b|\bvar\b|\blocal_var\b|\bglobal_var\b
      scope: storage.type.scope
      push:
        - include: line_ending_space
        - match: ':'
          push:
            - match: \n|\s
              pop: 2
            - match: (#).*$\n?
              scope: comment.line
              pop: 2
            - match: \$
              scope: keyword.dolla
              push:
                - include: line_ending
                - match: ({{valid_variable}})
                  scope: variable.language.at.parameter
                - match: \$
                  scope: keyword.dolla
                  pop: 1
            - match: ({{valid_variable}})
              scope: variable.parameter.scope.usage
              push:
                - match: (#).*$\n?
                  scope: comment.line
                  pop: 4
                - match: \$
                  scope: keyword.dolla
                  push:
                    - include: line_ending
                    - match: ({{valid_variable}})
                      scope: variable.language.at.parameter
                    - match: \$
                      scope: keyword.dolla
                      pop: 4
                - match: \n|\s|\.
                  pop: 3

  remove-var:
    - match: remove_(local_|global_)?variable
      scope: keyword.effect
      push:
        - match: =
          push:
            - match: \n
              pop: 2
            - match: (#).*$\n?
              scope: comment.line
              pop: 2
            - match: \$
              scope: keyword.dolla
              push:
                - include: line_ending
                - match: ({{valid_variable}})
                  scope: variable.language.at.parameter
                - match: \$
                  scope: keyword.dolla
                  pop: 1
            - match: ({{valid_variable}})
              scope: variable.parameter.remove.var
              push:
                - include: auto-generated-content
                - match: (#).*$\n?
                  scope: comment.line
                  pop: 4
                - match: \$
                  scope: keyword.dolla
                  push:
                    - include: line_ending
                    - match: ({{valid_variable}})
                      scope: variable.language.at.parameter
                    - match: \$
                      scope: keyword.dolla
                      pop: 4
                - match: \n
                  pop: 3

  save-scope:
    - match: save_scope_as
      scope: keyword.effect
      push:
        - match: =
          push:
            - match: \n
              pop: 2
            - match: (#).*$\n?
              scope: comment.line
              pop: 2
            - match: \$
              scope: keyword.dolla
              push:
                - include: line_ending
                - match: ({{valid_variable}})
                  scope: variable.language.at.parameter
                - match: \$
                  scope: keyword.dolla
                  pop: 1
            - match: ({{valid_variable}})
              scope: entity.name.function.scope.declaration
              push:
                - include: auto-generated-content
                - match: (#).*$\n?
                  scope: comment.line
                  pop: 4
                - match: \$
                  scope: keyword.dolla
                  push:
                    - include: line_ending
                    - match: ({{valid_variable}})
                      scope: variable.language.at.parameter
                    - match: \$
                      scope: keyword.dolla
                      pop: 4
                - match: \n
                  pop: 3

  one-line-var-trigger:
    - match: has_variable|has_global_variable
      scope: string.trigger
      push:
        - match: =
          push:
            - match: \n
              pop: 2
            - match: (#).*$\n?
              scope: comment.line
              pop: 2
            - match: \$
              scope: keyword.dolla
              push:
                - include: line_ending
                - match: ({{valid_variable}})
                  scope: variable.language.at.parameter
                - match: \$
                  scope: keyword.dolla
                  pop: 1
            - match: ({{valid_variable}})
              scope: variable.parameter.trigger.usage
              push:
                - include: auto-generated-content
                - match: (#).*$\n?
                  scope: comment.line
                  pop: 4
                - match: \$
                  scope: keyword.dolla
                  push:
                    - include: line_ending
                    - match: ({{valid_variable}})
                      scope: variable.language.at.parameter
                    - match: \$
                      scope: keyword.dolla
                      pop: 4
                - match: \n
                  pop: 3

  variable-definition-complex:
    - match: set_(local_|global_)?variable
      scope: keyword.effect
      push:
        - match: =\s?\{
          push:
            - match: (#).*$\n?
              scope: comment.line
            - match: \}
              pop: 3
            - match: name
              scope: constant.numeric
              push:
                - match: \n
                  pop: 3
                - match: (#).*$\n?
                  scope: comment.line
                  pop: 3
                - match: \$
                  scope: keyword.dolla
                  push:
                    - include: line_ending
                    - match: ({{valid_variable}})
                      scope: variable.language.at.parameter
                    - match: \$
                      scope: keyword.dolla
                      pop: 1
                - match: ({{valid_variable}})
                  scope: entity.name.function.var.declaration
                  push:
                    - include: auto-generated-content
                    - match: (#).*$\n?
                      scope: comment.line
                      pop: 4
                    - match: \$
                      scope: keyword.dolla
                      push:
                        - include: line_ending
                        - match: ({{valid_variable}})
                          scope: variable.language.at.parameter
                        - match: \$
                          scope: keyword.dolla
                          pop: 5
                    - match: \n
                      pop: 4
        - match: =
          push:
            - match: \n
              pop: 2
            - include: scripted-effect-zone
            - match: ({{valid_variable}})
              scope: entity.name.function.var.declaration
              pop: 2
        - match: \n
          pop: 2

  variable-usage-complex:
    - match: (change|round|clamp)_(local_|global_)?variable
      scope: keyword.effect
      push:
        - include: line_ending
        - match: = \{\n?+
          push:
            - match: (#).*$\n?
              scope: comment.line
            - match: \}
              pop: 3
            - match: name
              scope: constant.numeric
              push:
                - match: \n
                  pop: 3
                - match: (#).*$\n?
                  scope: comment.line
                  pop: 3
                - match: \$
                  scope: keyword.dolla
                  push:
                    - include: line_ending
                    - match: ({{valid_variable}})
                      scope: variable.language.at.parameter
                    - match: \$
                      scope: keyword.dolla
                      pop: 1
                - match: ({{valid_variable}})
                  scope: variable.parameter.trigger.usage
                  push:
                    - include: auto-generated-content
                    - match: (#).*$\n?
                      scope: comment.line
                      pop: 4
                    - match: \$
                      scope: keyword.dolla
                      push:
                        - include: line_ending
                        - match: ({{valid_variable}})
                          scope: variable.language.at.parameter
                        - match: \$
                          scope: keyword.dolla
                          pop: 5
                    - match: \n
                      pop: 4
        - match: \n
          pop: 2

  trigger-block-start:
    - match: \b(visible|cancellation_trigger|should_auto_expand|country_type_change_alternatives|can_change_subject_type_from|valid_overlord_country_types|valid_subject_country_types|valid_overlord_ranks|valid_subject_ranks|can_change_subject_type_from|valid_overlord_country_types|unlocking_laws|valid_for_country|available_for_interest_group|is_visible|disallowing_laws|possible_political_movements|change_allowed_trigger|invalid|fail|is_shown_when_inactive|complete|pop_potential|ideologies|enable|will_propose|selectable_in_lens|will_break|additional_involvement_trigger|is_about_to_auto_break|should_auto_break|should_invalidate|potential|trigger|limit|possible|happened|valid|unlocking_technologies|production_method_groups|is_shown)\b
      scope: string.trigger.block.start
      push:
        - include: line_ending
        - match: \{
          scope: meta.trigger.bracket
          pop: true

  effect-block-start:
    - match: \b(effect|on_activate|on_timeout|on_fail|on_complete|on_character_ig_membership|on_enable|on_disable|weekly_effect|break_effect|accept_effect|when_taken|immediate|option)\b
      scope: keyword.effect.block.start
      push:
        - include: line_ending
        - match: \{
          scope: meta.effect.bracket
          pop: true

  ai-block-start:
    - match: \b(ai_will_do|ai_chance|ai_weight)\b
      scope: string.ai.block.start
      push:
        - include: line_ending
        - match: \{
          scope: meta.ai.bracket
          pop: true

  bg-block-start:
    - match: \b(remaining_undepleted)\b
      scope: string.trigger
      push:
        - include: line_ending
        - match: \{
          scope: meta.bg.bracket
          pop: true

  char-trait-block-start:
    - match: \b(create_character)\b
      scope: keyword.effect
      push:
        - include: line_ending
        - match: \{
          scope: meta.char.trait.bracket
          pop: true

  at-symbol:
    - match: (@)({{valid_variable}})
      scope: keyword.numeric.at.symbol
      push:
        - match: (\[|\]|=|\s)
          scope: punctuation.definition.end
          pop: true

  at-symbol-expanded:
    - match: (@)\[
      scope: keyword.numeric.at.symbol
      push:
        - meta_content_scope: variable.language
        - match: \]
          scope: keyword.numeric.at.symbol
          pop: true

  scripted-trigger-event-declaration:
    - match: scripted_trigger
      scope: string.trigger.event.declaration
      push:
        - match: ({{valid_variable}})
          scope: variable.parameter.trigger.declaration
          pop: true

  scripted-effect-event-declaration:
    - match: scripted_effect
      scope: keyword.effect.event.declaration
      push:
        - match: ({{valid_variable}})
          scope: variable.parameter.effect.declaration
          pop: true

  line_ending:
    - include: comments
    - match: $\n
      pop: true

  line_ending_space:
    - include: comments
    - match: $\n|\s
      pop: 2

  manually-entered-content:
    # Essential Scipt Functions
    - match: \b(triggered_desc|minor_left_icon|minor_right_icon|right_icon|namespace|parent_group|default_building|chance|mode|adjective|default|popup_name|male_ruler|female_ruler|male_heir|female_heir|ethnicities|ai_weight|group|building_group|entity_not_constructed|city_gfx_interactions|entity_under_construction|entity_constructed|gui_window|left_icon|base|progress|status_desc|progress_desc|how_tutorial|why_tutorial|timeout|weight|first_name|last_name|texture|icon|placement|title|desc|flavor|event_image|duration|default_option|months|days|years|hidden|id|text|log_loc_errors|name|country_type|tier|flag_definition|allow_overlord_canton|order_by|qualifications_growth_desc)\b
      scope: constant.numeric
    # Math/Sound/Video

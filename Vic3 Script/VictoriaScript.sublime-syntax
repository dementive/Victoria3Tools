  variable-definition-complex:
    - match: set_(local_|global_)?variable
      scope: keyword.effect
      push:
        - match: =
          scope: keyword.operator.comparison.victoria
        - match: \{\n?+
          push:
            - match: (#).*$\n?
              scope: comment.line
            - match: \}
              pop: 3
            - match: name
              scope: constant.numeric
              push:
                - match: =
                  scope: keyword.operator.comparison.victoria
                - match: \n
                  pop: 3
                - match: (#).*$\n?
                  scope: comment.line
                  pop: 3
                - match: \$
                  scope: keyword.dolla
                  push:
                    - include: line_ending
                    - match: ({{valid_variable}})
                      scope: variable.language.at.parameter
                    - match: \$
                      scope: keyword.dolla
                      pop: 1
                - match: ({{valid_variable}})
                  scope: entity.name.function.var.declaration
                  push:
                    - include: auto-generated-content
                    - match: (#).*$\n?
                      scope: comment.line
                      pop: 4
                    - match: \$
                      scope: keyword.dolla
                      push:
                        - include: line_ending
                        - match: ({{valid_variable}})
                          scope: variable.language.at.parameter
                        - match: \$
                          scope: keyword.dolla
                          pop: 5
                    - match: \n
                      pop: 4
        - match: =
          scope: keyword.operator.comparison.victoria
          push:
            - match: \n
              pop: 2
            - include: scripted-effect-zone
            - match: ({{valid_variable}})
              scope: entity.name.function.var.declaration
              pop: 2
        - match: \n
          pop: 2

  variable-usage-complex:
    - match: (change|round|clamp)_(local_|global_)?variable
